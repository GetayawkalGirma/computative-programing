class Solution: 
    def select(self, arr, i):
        # code here 
        for i in range(len(arr)):
            print(arr[i])
    
    def selectionSort(self, arr,n):
        #code here
        for i in range(n):
            for j in range(i+1,n):
                if arr[j] <arr[i]:
                    temp=arr[j]
                    arr[j]=arr[i]
                    arr[i]=temp
    
